generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model pessoaList {
  id       Int    @id @default(autoincrement())
  cpf      String @unique @db.VarChar(11)
  email    String @unique @db.VarChar(150)
  password String @db.VarChar(200)
  name     String @db.VarChar(255)
}
